<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearTab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="30dp"
        android:background="#DCD9D9"
        android:padding="20dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="Position" />

            <TextView
                android:id="@+id/tvPosLeader1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="1 -" />

            <TextView
                android:id="@+id/tvPosLeader2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="2 -" />

            <TextView
                android:id="@+id/tvPosLeader3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="3 -" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="Personnage" />

            <TextView
                android:id="@+id/tvChaLeader1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/tvChaLeader2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/tvChaLeader3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="Nombre de coups" />

            <TextView
                android:id="@+id/tvCoupLeader1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/tvCoupLeader2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/tvCoupLeader3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="8dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="Temps(s)" />

            <TextView
                android:id="@+id/tvTempsLeader1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/tvTempsLeader2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/tvTempsLeader3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginVertical="8dp"
                android:layout_weight="0.25"
                android:gravity="center"
                android:text="TextView" />

        </LinearLayout>

    </LinearLayout>
</LinearLayout>